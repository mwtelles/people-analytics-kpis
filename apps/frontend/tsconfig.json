{
    "compilerOptions": {
        "target": "ES2020",
        "module": "Node16",
        "jsx": "react-jsx",
        "moduleResolution": "node16",
        "ignoreDeprecations": "6.0",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "types": [
            "vitest/globals"
        ]
    },
    "include": [
        "src",
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}